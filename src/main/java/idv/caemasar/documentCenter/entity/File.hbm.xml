<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="idv.caemasar.documentCenter.entity.File" table="dc_t_files">
		<id name="flie_id" column="flie_id" type="int">
			<generator class="increment" />
		</id>
		<property name="file_userid" column="file_userid" />
		<property name="file_filename" column="file_filename" />
		<property name="file_path" column="file_path" />
		<property name="file_size" column="file_size" />
		<property name="file_uploadtime" column="file_uploadtime" />
		<property name="file_type" column="file_type" />
	</class> 
</hibernate-mapping>
