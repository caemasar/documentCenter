<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="idv.caemasar.documentCenter.entity.User" table="dc_t_users">
		<id name="u_id" column="u_id" />
		<property name="u_username" column="u_username" />
		<property name="u_password" column="u_password" />
		<property name="u_realname" column="u_realname" />
		<property name="u_email" column="u_email" />
		<property name="u_phone" column="u_phone" />
		<property name="u_deptid" column="u_deptid" />
		<property name="u_identity" column="u_identity" />
	</class>
</hibernate-mapping>
 